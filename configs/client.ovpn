setenv CLIENT_CERT 0
client
dev tun
proto udp
remote $vpn_server_host $vpn_server_port
resolv-retry infinite
nobind
persist-key
persist-tun
<ca>
$ca_certificate
</ca>
key-direction 1 # needed when tls-auth is included in config-file
# openvpn --genkey --secret ta.key
<tls-auth>
$ta_key
</tls-auth>
cipher $cipher
auth-user-pass
#ns-cert-type server
verb 3
