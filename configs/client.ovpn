setenv CLIENT_CERT 0
client
dev tun
proto udp
remote <vpn-server-ip/dns> 1194
resolv-retry infinite
nobind
persist-key
persist-tun
<ca>
-----BEGIN CERTIFICATE-----
...
-----END CERTIFICATE-----
</ca>
key-direction 1 # needed when tls-auth is included in config-file
# openvpn --genkey --secret ta.key
<tls-auth>
-----BEGIN OpenVPN Static key V1-----
...
-----END OpenVPN Static key V1-----
</tls-auth>
cipher AES-256-CBC
auth-user-pass
#ns-cert-type server
verb 3
